          </geometry>
          <material name="lidar_top" />
      </visual>
  </link>
  
  <joint name="lidar_internal_joint" type="fixed">
    <parent link="lidar_base" />
    <child link="lidar_top" />
    <origin xyz="0 0 -0.01925" rpy="0 0 0"/>
